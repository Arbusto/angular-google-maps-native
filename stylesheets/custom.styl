html
  font-size 100%

ol
  li + li
    margin-top 30px

nav.navbar
  min-height 0
  #navbar-collapse
    line-height 10px
    text-align center
    >ul.navbar-nav
      display inline-block
      float none
      padding 0
      >li
        font-size 13px
        >a
          padding 10px 15px
          i
            margin-right 5px
    .divider-vertical
      width 1px
      height 40px
      margin 0
      overflow hidden
      background-color #222222
      border-right 1px solid #333333

snippets-tabs
  .btn
    font-size 14px
    padding 6px 12px
    &:hover
      color #000
      background-color #fff
      border: 1px solid #ddd
    &.active
      outline 0
      background-image none
      box-shadow inset 0 3px 5px rgba(0,0,0,.125)

.tab-pane
  iframe
    width 100%
    height 400px
    border 0
    background-color #F9F9F9

pre.code
  font-size 12px